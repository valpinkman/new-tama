{
  "version": 2,
  "alias": ["tamarindo-v2"],
  "github": {
    "autoJobCancelation": true
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/notre-concept",
      "dest": "/about"
    },
    {
      "src": "/nos-produits/(?<product>[^/]+)",
      "dest": "/products/$product"
    },
    {
      "src": "/nos-produits",
      "dest": "/products"
    },
    {
      "src": "/fraiches-news",
      "dest": "/blog"
    },
    {
      "src": "/fraiches-news/fraiche/(?<slug>[^/]+)",
      "dest": "/blog/article/$slug"
    },
    {
      "src": "/boutique",
      "dest": "https://shop.smoothiestamarindo.com"
    },
    {
      "src": "/espace-pro",
      "dest": "/pro"
    },
    {
      "src": "/connexion",
      "dest": "/signin"
    },
    {
      "src": "/inscription",
      "dest": "/signup"
    },
    {
      "src": "/our-concept",
      "dest": "/about"
    },
    {
      "src": "/our-products/(?<product>[^/]+)",
      "dest": "/products/$product"
    },
    {
      "src": "/our-products",
      "dest": "/products"
    },
    {
      "src": "/fresh-news",
      "dest": "/blog"
    },
    {
      "src": "/fresh-news/fresh/(?<slug>[^/]+)",
      "dest": "/blog/article/$slug"
    },
    {
      "src": "/shop",
      "dest": "https://shop.smoothiestamarindo.com"
    },
    {
      "src": "/concetto",
      "dest": "/about"
    },
    {
      "src": "/prodotti",
      "dest": "/products"
    },
    {
      "src": "/prodotti/(?<prodcut>[^/]+)",
      "dest": "/products/$prodcut"
    },
    {
      "src": "/notizie-fresche",
      "dest": "/blog"
    },
    {
      "src": "/notizie-fresche/fresca/(?<slug>[^/]+)",
      "dest": "/blog/article/$slug"
    },
    {
      "src": "/negozio",
      "dest": "https://shop.smoothiestamarindo.com"
    },
    {
      "src": "/sezione-pro",
      "dest": "/pro"
    },
    {
      "src": "/connessione",
      "dest": "/signin"
    },
    {
      "src": "/iscrizione",
      "dest": "/signup"
    }
  ]
}
